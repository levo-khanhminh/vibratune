{
  "expo": {
    "name": "vibratune",
    "slug": "vibratune",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "vibratune",
    "splash": {
      "image": "./assets/images/vibratune-logo-2.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#0A1A2F"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo-3.jpg",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.khanhminh.vibratune",
      "plugins": [
        [
          "expo-build-properties",
          {
            "android": {
              "kotlinVersion": "1.9.22",
              "compileSdkVersion": 35,
              "targetSdkVersion": 34,
              "buildToolsVersion": "35.0.0"
            }
          }
        ]
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "cli": {
      "appVersionSource": "local"
    },
    "plugins": ["expo-router"],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "23e80df9-9cfb-4716-abb8-3cf14f3cb2bf"
      }
    }
  }
}
